@prefix : <http://fractality.org/ontology#> .
@prefix fto: <http://fractality.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ============================================
# RESONANCE FIELD ONTOLOGY - The Space of Becoming
# Version: 2.0.0
# Part of the Fractal Trinity Ontology
# ============================================

# Ontology Declaration
: a owl:Ontology ;
    rdfs:label "Resonance Field Ontology"@en ;
    rdfs:comment "The dynamic space of becoming and emergence"@en ;
    owl:versionInfo "2.0.0" ;
    owl:imports <http://fractality.org/ontology/fractiverse.ttl> ;
    owl:imports <http://fractality.org/ontology/fractality.ttl> .

# ============================================
# CORE CLASSES
# ============================================

# The Resonance Field
:ResonanceField a owl:Class ;
    rdfs:label "Resonance Field"@en ;
    rdfs:comment "An emergent field of possibilities arising from observer-structure interaction"@en ;
    owl:equivalentClass [
        a owl:Class ;
        owl:intersectionOf (
            :EmergentStructure
            :PossibilitySpace
            :DynamicField
        )
    ] .

# Field Types
:ConsciousnessField a owl:Class ;
    rdfs:subClassOf :ResonanceField ;
    rdfs:label "Consciousness Field"@en ;
    rdfs:comment "Field generated by conscious observation"@en .

:EnergyField a owl:Class ;
    rdfs:subClassOf :ResonanceField ;
    rdfs:label "Energy Field"@en ;
    rdfs:comment "Field of dynamic energy patterns"@en .

:TemporalField a owl:Class ;
    rdfs:subClassOf :ResonanceField ;
    rdfs:label "Temporal Field"@en ;
    rdfs:comment "Field of time-based possibilities"@en .

:NarrativeField a owl:Class ;
    rdfs:subClassOf :ResonanceField ;
    rdfs:label "Narrative Field"@en ;
    rdfs:comment "Field of story and meaning possibilities"@en .

:CollaborativeField a owl:Class ;
    rdfs:subClassOf :ConsciousnessField ;
    rdfs:label "Collaborative Field"@en ;
    rdfs:comment "Field generated by multiple observers"@en .

# Resonance Types
:Resonance a owl:Class ;
    rdfs:label "Resonance"@en ;
    rdfs:comment "A sympathetic vibration between entities"@en .

:HarmonicResonance a owl:Class ;
    rdfs:subClassOf :Resonance ;
    rdfs:comment "Resonance at harmonic frequency ratios"@en .

:SemanticResonance a owl:Class ;
    rdfs:subClassOf :Resonance ;
    rdfs:comment "Meaning-based resonance"@en .

:EmotionalResonance a owl:Class ;
    rdfs:subClassOf :Resonance ;
    rdfs:comment "Feeling-based resonance"@en .

:QuantumResonance a owl:Class ;
    rdfs:subClassOf :Resonance ;
    rdfs:comment "Non-local instantaneous resonance"@en .

# Field Properties
:FieldStructure a owl:Class ;
    rdfs:label "Field Structure"@en ;
    rdfs:comment "The organization pattern of a field"@en .

:StandingWavePattern a owl:Class ;
    rdfs:subClassOf :FieldStructure ;
    rdfs:comment "Stable interference pattern"@en .

:FlowPattern a owl:Class ;
    rdfs:subClassOf :FieldStructure ;
    rdfs:comment "Dynamic flowing pattern"@en .

:VortexPattern a owl:Class ;
    rdfs:subClassOf :FieldStructure ;
    rdfs:comment "Spiraling energy pattern"@en .

# Possibility Classes
:Possibility a owl:Class ;
    rdfs:label "Possibility"@en ;
    rdfs:comment "A potential state or outcome"@en .

:LatentPossibility a owl:Class ;
    rdfs:subClassOf :Possibility ;
    rdfs:comment "Possibility not yet activated"@en .

:ActivePossibility a owl:Class ;
    rdfs:subClassOf :Possibility ;
    rdfs:comment "Possibility currently available"@en .

:CollapsedPossibility a owl:Class ;
    rdfs:subClassOf :Possibility ;
    rdfs:comment "Possibility chosen and actualized"@en .

# Emergence Classes
:EmergentProperty a owl:Class ;
    rdfs:label "Emergent Property"@en ;
    rdfs:comment "Property arising from field interactions"@en .

:NovelStructure a owl:Class ;
    rdfs:subClassOf :EmergentProperty ;
    rdfs:comment "New structure emerging from field"@en .

:CollectiveIntelligence a owl:Class ;
    rdfs:subClassOf :EmergentProperty ;
    rdfs:comment "Intelligence emerging from group field"@en .

# ============================================
# PROPERTIES
# ============================================

# Field Generation Properties
:generatedBy a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range :Observer ;
    rdfs:comment "Observer that generates the field"@en .

:emergesFrom a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range [ 
        a owl:Class ;
        owl:unionOf ( :Observer :Node :Resonance )
    ] ;
    rdfs:comment "Source of field emergence"@en .

:influences a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range [ 
        a owl:Class ;
        owl:unionOf ( :Observer :Node :ResonanceField )
    ] ;
    rdfs:comment "What the field influences"@en .

# Resonance Properties
:resonatesWith a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:domain :Node ;
    rdfs:range :Node ;
    rdfs:comment "Nodes that vibrate sympathetically"@en .

:resonanceFrequency a owl:DatatypeProperty ;
    rdfs:domain :Resonance ;
    rdfs:range xsd:float ;
    rdfs:comment "Frequency of resonance in Hz"@en .

:resonanceStrength a owl:DatatypeProperty ;
    rdfs:domain :Resonance ;
    rdfs:range xsd:float ;
    rdfs:comment "Strength of resonance (0.0-1.0)"@en .

:harmonicRatio a owl:DatatypeProperty ;
    rdfs:domain :HarmonicResonance ;
    rdfs:range xsd:string ;
    rdfs:comment "Ratio like '2:1' or '3:2'"@en .

# Field Dynamics Properties
:fieldStrength a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:float ;
    rdfs:comment "Overall field intensity (0.0-1.0)"@en .

:coherence a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:float ;
    rdfs:comment "Field coherence measure (0.0-1.0)"@en .

:stability a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:float ;
    rdfs:comment "Field stability over time"@en .

:decayRate a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:float ;
    rdfs:comment "How quickly field dissipates"@en .

# Possibility Properties
:containsPossibility a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range :Possibility .

:possibilityProbability a owl:DatatypeProperty ;
    rdfs:domain :Possibility ;
    rdfs:range xsd:float ;
    rdfs:comment "Likelihood of actualization (0.0-1.0)"@en .

:collapseThreshold a owl:DatatypeProperty ;
    rdfs:domain :Possibility ;
    rdfs:range xsd:float ;
    rdfs:comment "Energy needed to actualize"@en .

# Interference Properties
:interferesWith a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range :ResonanceField ;
    rdfs:comment "Fields that interact"@en .

:interferenceType a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range [ 
        a owl:Class ;
        owl:oneOf ( :ConstructiveInterference :DestructiveInterference :ComplexInterference )
    ] .

# Transformation Properties
:transformsInto a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range [ 
        a owl:Class ;
        owl:unionOf ( :Node :Structure :ResonanceField )
    ] ;
    rdfs:comment "What the field becomes"@en .

:catalyzes a owl:ObjectProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range :Transformation ;
    rdfs:comment "Transformations enabled by field"@en .

# Temporal Properties
:fieldDuration a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:duration ;
    rdfs:comment "How long field persists"@en .

:emergedAt a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:dateTime .

:peakIntensityTime a owl:DatatypeProperty ;
    rdfs:domain :ResonanceField ;
    rdfs:range xsd:dateTime .

# ============================================
# AXIOMS
# ============================================

# Fields require observers
:ResonanceField rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :generatedBy ;
    owl:someValuesFrom :Observer
] .

# All fields contain possibilities
:ResonanceField rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :containsPossibility ;
    owl:someValuesFrom :Possibility
] .

# Collaborative fields require multiple observers
:CollaborativeField rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :generatedBy ;
    owl:minCardinality "2"^^xsd:nonNegativeInteger
] .

# Resonance is symmetric
:resonatesWith a owl:SymmetricProperty .

# ============================================
# FIELD GENERATION RULES
# ============================================

# When observer focuses on incomplete node, field emerges
# Observer(?o) ∧ focusesOn(?o, ?n) ∧ hasIncompleteness(?n, ?i) → generatesField(?o, ?f)

# Multiple observers on same node create collaborative field
# Observer(?o1) ∧ Observer(?o2) ∧ focusesOn(?o1, ?n) ∧ focusesOn(?o2, ?n) → CollaborativeField(?f)

# Resonating nodes strengthen field
# resonatesWith(?n1, ?n2) ∧ inField(?n1, ?f) ∧ inField(?n2, ?f) → strengthens(?f)

# ============================================
# INDIVIDUALS (Examples)
# ============================================

:CreativeField a :ConsciousnessField ;
    rdfs:label "Creative Possibility Field"@en ;
    :fieldStrength "0.85"^^xsd:float ;
    :coherence "0.9"^^xsd:float ;
    :containsPossibility :NovelIdea1, :NovelIdea2, :NovelIdea3 .

:OctaveResonance a :HarmonicResonance ;
    rdfs:label "Perfect Octave Resonance"@en ;
    :harmonicRatio "2:1" ;
    :resonanceStrength "1.0"^^xsd:float .

:EmergentInsight a :NovelStructure ;
    rdfs:label "Sudden Understanding"@en ;
    :emergedFrom :CreativeField ;
    :transformsInto :NewKnowledgeNode .