.jobsList .horiz li.last, .jobsList li.last, .highlight li:last-child { padding: 0px; border: 0px; }

.highlight li:last-child { margin: 0px; }

.jobsList .recruiterDetails li, .jobsList p.apply { float: left; margin: 0px 10px 0px 0px; padding: 0px; color: rgb(119, 119, 119); font-size: 11px; border: 0px; }

.jobsList p.apply { float: right; margin: 0px; }

.jobsList .logoWrap { float: right; text-align: right; width: 125px; }

.jobsList .premium { display: none; }

.moreJobs { margin: 20px 0px 0px; }

.moreJobs a { display: block; padding: 5px 10px; background: rgb(51, 51, 51); color: rgb(255, 255, 255); text-align: right; font-weight: bold; border-radius: 5px; }

.cf::before, .cf::after { content: " "; display: block; height: 0px; visibility: hidden; }

.cf::after { clear: both; }

.cf { zoom: 1; }

html { box-sizing: border-box; }

a { color: rgb(56, 56, 56); font-weight: bold; text-decoration: none; }

a:hover { color: gray; }

body { color: rgb(102, 102, 102); font-size: 13px; font-family: "Benton Sans", "Helvetica Neue", Helvetica, Arial, sans-serif; -webkit-font-smoothing: antialiased; overflow: hidden; padding: 0px; margin: 0px; }

.jobsList { position: relative; padding: 0px; box-sizing: border-box; overflow: hidden; -webkit-font-smoothing: antialiased; color: rgb(102, 102, 102); display: block; font-family: Roboto, sans-serif; font-size: 12px; font-style: normal; font-variant-caps: normal; font-weight: normal; line-height: 13px; list-style: outside none none; margin: 15px 0px 0px; text-align: left; overflow-wrap: break-word; }

.jobsList::before { position: relative; display: block; color: rgb(202, 32, 21); font-size: 24px; font-weight: 700; font-family: Roboto, sans-serif; letter-spacing: -1px; text-align: left; margin: 5px 0px 10px; white-space: nowrap; }

.jobsList h4 { margin: 0px 0px 5px; -webkit-font-smoothing: antialiased; background-color: rgba(0, 0, 0, 0); box-sizing: border-box; color: rgb(128, 128, 128); cursor: auto; font-family: Roboto, sans-serif; font-size: 16px; font-style: normal; font-variant-caps: normal; font-weight: bold; height: auto; line-height: 19px; list-style: outside none none; outline: rgb(128, 128, 128) none 0px; text-align: left; text-decoration: none; width: auto; overflow-wrap: break-word; }

.jobsList ul { margin: 0px; padding: 0px; list-style: none; }

.jobsList li { border-top: none; border-bottom: 1px solid rgb(230, 230, 230); font-size: 11px; overflow: hidden; text-overflow: ellipsis; padding-bottom: 5px; margin-bottom: 5px; padding-top: 5px; margin-top: 5px; }

.jobsList .horiz li.last, .jobsList li.last, .highlight li:last-child { padding: 0px; margin: 0px; border-top: none; }

.jobsList li.regular { }

.jobsList .logoWrap { float: none; display: block; width: auto; }

.jobsList .logoWrap img { display: block; margin: 5px auto; }

.jobsList ul strong { font-weight: normal; }

.adBody p { display: none; }

.premium, .logo, .apply { display: none; }

.jobsList > li:first-of-type { padding-top: 0px; }

.jobsList > li:last-of-type { border-bottom: 0px; margin-bottom: 0px; padding-bottom: 0px; }

.moreJobs { margin-top: 10px; }

.moreJobs a { display: block; padding: 0px 10px 5px; background: transparent; color: rgb(56, 56, 56); font-size: 11px; text-align: left; text-transform: uppercase; }

.jobsList .logoWrap, .jobsList .logoWrap img { display: none; }
